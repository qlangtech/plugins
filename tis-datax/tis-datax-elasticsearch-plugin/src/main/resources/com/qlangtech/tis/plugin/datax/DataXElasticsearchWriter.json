{
  "endpoint": {
    "help": "ElasticSearch的连接地址",
    "creator": {
      "label": "管理",
      "plugin": [
        {
          "hetero": "params-cfg",
          "descName": "elasticToken"
        }
      ]
    }
  },
  "index": {
    "help": "Elasticsearch中的index名",
    "dftVal": "com.qlangtech.tis.plugin.datax.SelectedTab.getDftTabName():uncache_true"
  },
  "cleanup": {
    "help": "是否删除原表",
    "dftVal": false,
    "enum": [
      {
        "val": true,
        "label": "是"
      },
      {
        "val": false,
        "label": "否"
      }
    ]
  },
  "batchSize": {
    "help": "每次批量数据的条数",
    "dftVal": 1000
  },
  "trySize": {
    "help": "失败后重试的次数",
    "dftVal": 30
  },
  "timeout": {
    "help": "客户端超时时间",
    "dftVal": 600000
  },
  "discovery": {
    "help": "启用节点发现将(轮询)并定期更新客户机中的服务器列表",
    "dftVal": false,
    "enum": [
      {
        "val": true,
        "label": "是"
      },
      {
        "val": false,
        "label": "否"
      }
    ]
  },
  "compression": {
    "help": "http请求，开启压缩",
    "dftVal": true,
    "enum": [
      {
        "val": true,
        "label": "是"
      },
      {
        "val": false,
        "label": "否"
      }
    ]
  },
  "multiThread": {
    "help": "http请求，是否有多线程",
    "dftVal": true,
    "enum": [
      {
        "val": true,
        "label": "是"
      },
      {
        "val": false,
        "label": "否"
      }
    ]
  },
  "ignoreWriteError": {
    "label": "忽略错误",
    "help": "忽略写入错误，不重试，继续写入",
    "dftVal": false,
    "enum": [
      {
        "val": true,
        "label": "是"
      },
      {
        "val": false,
        "label": "否"
      }
    ]
  },
  "ignoreParseError": {
    "help": "忽略解析数据格式错误，继续写入",
    "dftVal": true,
    "enum": [
      {
        "val": true,
        "label": "是"
      },
      {
        "val": false,
        "label": "否"
      }
    ]
  },
  "alias": {
    "help": "数据导入完成后写入别名"
  },
  "aliasMode": {
    "help": "数据导入完成后增加别名的模式，append(增加模式), exclusive(只留这一个)",
    "dftVal": "append",
    "enum": [
      {
        "val": "exclusive",
        "label": "Exclusive"
      },
      {
        "val": "append",
        "label": "Append"
      }
    ]
  },
  "settings": {
    "dftVal": "{\"index\" :{\"number_of_shards\": 1, \"number_of_replicas\": 0}}",
    "style": "codemirror",
    "mode": "application/ld+json"
  },
  "splitter": {
    "help": "如果插入数据是array，就使用指定分隔符",
    "dftVal": ","
  },
  "dynamic": {
    "help": "不使用datax的mappings，使用es自己的自动mappings",
    "dftVal": false,
    "enum": [
      {
        "val": true,
        "label": "是"
      },
      {
        "val": false,
        "label": "否"
      }
    ]
  },
  "template": {
    "dftVal": "com.qlangtech.tis.plugin.datax.DataXElasticsearchWriter.getDftTemplate()"
  }
}
